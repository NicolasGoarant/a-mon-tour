<%= simple_form_for [@newspaper, @article] do |f| %>
  <%= f.input :title %>
  <%= f.input :publication %>
  <%= f.input :photo %>
  <%= f.input :media %>
  <%= f.input :content %>
  <%= f.input :address %>
  <%= f.input :engagement %>


  <%= f.submit "Ajouter cet article", class: "btn btn-primary" %>
<% end %>


